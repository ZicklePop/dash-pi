{
    "parser": "babel-eslint",
    "plugins": ["react"],
    "env": {
        "jest": true,
        "browser": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "array-bracket-spacing": "off",
        "block-spacing": ["error", "always"],
        "brace-style": ["error", "1tbs"],
        "callback-return": "warn",
        "camelcase": "warn",
        "comma-dangle": [2, "only-multiline"],
        "comma-spacing": "error",
        "comma-style": ["error", "first", {
            "exceptions": { "ObjectExpression": true, "ArrayExpression": true }
        }],
        "complexity": ["warn", { "max": 9 }],
        "computed-property-spacing": ["error", "never"],
        "consistent-this": ["error", "self"],
        "eol-last": "error",
        "func-names": "warn",
        "func-style": ["error", "declaration", { "allowArrowFunctions":  true }],
        "handle-callback-err": "warn",
        "id-length": "off",
        "id-match": "off",
        "indent": ["warn", 2, {"SwitchCase": 1}],
        "key-spacing": ["error", {"mode": "minimum"}],
        "keyword-spacing": ["error"],
        "linebreak-style": "error",
        "lines-around-comment": "error",
        "max-depth": ["warn", 10],
        "max-len": [2, 100],
        "max-nested-callbacks": ["warn", 3],
        "max-params": ["warn", 4],
        "max-statements": ["warn", 15],
        "new-cap": ["warn", {"capIsNewExceptions": ["Velocity"]}],
        "new-parens": "off",
        "newline-after-var": "off",
        "no-array-constructor": "off", // Redundant
        "no-bitwise": "warn",
        "no-console": [2, { allow: ["warn", "error"] }],
        "no-continue": "error",
        "no-debugger": 2,
        "no-inline-comments": "off",
        "no-lonely-if": "error",
        "no-mixed-requires": ["warn", true],
        "no-mixed-spaces-and-tabs": "error",
        "no-multiple-empty-lines": ["error", {"max": 2}],
        "no-nested-ternary": "warn",
        "no-new-object": "off", // Redundant
        "no-new-require": "error",
        "no-path-concat": "error",
        "no-plusplus": "warn",
        "no-process-exit": "error",
        "no-redeclare": 2,
        "no-spaced-func": "error",
        "no-sync": "warn",
        "no-ternary": "off",
        "no-trailing-spaces": "error",
        "no-underscore-dangle": "warn",
        "no-unneeded-ternary": "error",
        "no-useless-concat": "error",
        "object-curly-spacing": "off",
        "one-var": "off",
        "operator-assignment": ["error", "always"],
        "operator-linebreak": ["error", "before"],
        "padded-blocks": ["error", "never"],
        "prefer-const": 2,
        "quote-props": ["error", "as-needed"],
        "quotes": [2, "single"],
        "react/no-danger": "off",
        "react/no-deprecated": 2,
        "react/sort-comp": 2,
        "react/sort-prop-types": 2,
        "semi-spacing": "error",
        "semi": [2, "never"],
        "sort-vars": "off",
        "space-in-parens": "off",
        "space-infix-ops": "error",
        "space-unary-ops": "error",
        "spaced-comment":  ["error", "always", {exceptions: ["-", "+"]}],
        "wrap-regex": "off",
    }
}